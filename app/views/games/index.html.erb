<div class="row">
    <% @games.each do |game| %>
        <div class="col-md-3">
            <a href="/games/<%= game.id %>">
                <%= image_tag game.image.url, class: "game" %>
            </a>
        </div>
    <% end %>
</div>

<script src="../../assets/javascripts/jquery.raty.js"></script>
<script>
    $('.comment-rating').raty({
        readOnly: true,
        score: function() {
            return $(this).attr('data-score');
        },
        path: '/assets/'
    });
</script>